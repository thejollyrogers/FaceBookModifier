{
  "name": "FacebookModifier",
  "version": "1.0",
  "description": "Change your FaceBook layout. Delete and rearrange elements on your FaceBook page.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://www.facebook.com/*", "https://www.facebook.com", "http://facebook.com", "https://facebook.com"],
      "js": ["js/prototype.js", "js/main.js"]
    }
  ]
  
 }